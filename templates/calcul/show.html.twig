{% extends 'base-membre.html.twig' %}

{% block title %}Calcul{% endblock %}

{% block body %}
    <h1>Calcul</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ calcul.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ calcul.title }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ calcul.date ? calcul.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Devis</th>
                <td>{{ calcul.devis }}</td>
            </tr>
            <tr>
                <th>DurationMonth</th>
                <td>{{ calcul.durationMonth }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_calcul_index') }}">back to list</a>

    <a href="{{ path('app_calcul_edit', {'id': calcul.id}) }}">edit</a>

    {{ include('calcul/_delete_form.html.twig') }}
{% endblock %}
